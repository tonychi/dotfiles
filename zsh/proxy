# AgentWho config for shell

function start-agentwho() {
    export http_proxy="h1.agent02.net:22631"
    export https_proxy="h1.agent02.net:22631"
    echo "http proxy started..."
}

function stop-agentwho() {
    export http_proxy=""
    export https_proxy=""
    echo "http proxy stopped..."
}

# AgentWho config for shell END

